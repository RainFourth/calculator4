<resources xmlns:tools="http://schemas.android.com/tools">

    <!--Launch Screen-->
    <!-- Кастомная AppTheme.Launcher. Устанавливает в качестве фона вашу drawable -->
    <style name="AppThemeDark.Launcher">
        <!-- для Android 5+ вы можете поменять цвет colorPrimaryDark, чтобы соответствовать цвету windowBackground -->
        <item name="android:windowBackground">@drawable/launch_screen</item>
    </style>






    <!--СЮДА ВЫНЕСЕНЫ НАИБОЛЕЕ ОБЩИЕ АТРИБУТЫ СВЕТЛЫХ ТЕМ-->
    <style name="ThemeLightAbstract" parent="Theme.AppCompat.Light.NoActionBar">
        <!--задаёт высоту экшн бара-->
        <item name="actionBarSize">48dp</item>


        <item name="android:windowLightStatusBar" tools:targetApi="m">false</item>

        <item name="onColorPrimary">@color/white</item>

        <item name="android:textColor">@color/black</item>

        <item name="colorAccent">?colorPrimary</item>
        <item name="onColorAccent">@color/white</item>
        <item name="onColorAccentVariant">?onColorAccent</item>

        <item name="themeColor">?colorPrimary</item>
        <item name="onThemeColor">?onColorPrimary</item>

        <item name="android:colorControlNormal">@color/black</item>

        <item name="android:colorControlActivated">?themeColor</item>
        <item name="circleIndicatorColor">?themeColor</item>
        <item name="onCircleIndicatorColor">?onThemeColor</item>

        <item name="expressionBgc">@color/white</item>
        <item name="onExpressionBgc">@color/black</item>
        <item name="onExpressionBgcVariant">@color/gray2</item>

        <item name="resultBgc">?themeColor</item>
        <item name="onResultBgc">?onKbdThemeColor</item>
        <item name="resultColor">?onThemeColor</item>
        <item name="resultColorVariant">@color/light_gray_2</item>
        <item name="resultColorLand">?onExpressionBgc</item>

        <item name="kbdColor">?expressionBgc</item>
        <item name="onKbdColor">@color/gray</item>
        <item name="onKbdColorVariant">@color/gray</item>

        <item name="kbdThemeColor">?themeColor</item>
        <item name="onKbdThemeColor">?onThemeColor</item>

        <item name="historyPeekBgc">?themeColor</item>
        <item name="onHistoryPeekBgc">?onThemeColor</item>

        <item name="cardListColor">@color/light_gray_5</item>
        <item name="cardColor">@color/white</item>
        <item name="onCard">@color/black</item>

        <item name="android:navigationBarColor">?android:colorPrimaryDark</item>

        <item name="twoFieldsSeparator">@color/light_gray_2</item>

        <item name="toolbarBtnColor">@color/black</item>
        <item name="toolbarBtnColorLight">?android:colorPrimaryDark</item>

        <item name="onToolbarBtnColor">@color/white</item>
        <item name="onToolbarBtnColorLight">@color/white</item>
        <item name="onToolbarBtnColorWarn">@color/white</item>

    </style>



